    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-sort-export-all": "^1.2.2",
    "eslint-plugin-sort-exports": "^0.7.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "webpack": "^5.74.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write"
    ],
    "*.js": "eslint --fix"
  }
